{
  "name": "tippani",
  "productName": "Tippani - Evernote Client",
  "version": "1.0.1",
  "description": "An unofficial cross-platform client for Evernote Web",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . 'Evernote Web' --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . 'Evernote Web' --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "Kashinath Patekar",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.3"
  },
  "dependencies": {
    "darkreader": "^4.9.2"
  }
}
