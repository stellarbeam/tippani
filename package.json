{
  "name": "tippani",
  "productName": "Tippani - Evernote Client",
  "version": "1.0.1",
  "description": "An unofficial linux client for Evernote Web\n",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.stellarbeam.tippani",
    "productName": "Tippani - Evernote Client",
    "linux": {
      "category": "Utils",
      "icon": "assets/icons",
      "packageCategory": "utils",
      "maintainer": "Kashinath Patekar <kashinathpatekar26@gmail.com>",
      "target": [
        "deb"
      ],
      "synopsis": "Take notes with Evernote\n"
    }
  },
  "author": "Kashinath Patekar",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "arrive": "^2.4.1",
    "darkreader": "^4.9.2",
    "fs": "0.0.1-security"
  }
}
